{
  "name": "cider_utils",
  "version": "0.2.0",
  "description": "",
  "main": "index.js",
  "files": [
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ciderapp/cider_utils.git"
  },
  "scripts": {
    "install": "run-script-os",
    "install:win32": "prebuild-install -r napi -t 3 || yarn run rebuild",
    "install:linux:darwin": "prebuild-install -r napi -t 3 || yarn run rebuild",
    "build-binaries-electron": "run-script-os",
    "build-binaries-electron-ci": "prebuild --backend cmake-js --platform win32 --platform linux --platform darwin --include-regex \"^.*\\.(node|dylib|lib|exp|dll|so(\\.[0-9])?)$\" -r napi -t 3",
    "build-binaries-electron:win32": "prebuild --backend cmake-js --platform win32 --include-regex \"^.*\\.(node|dylib|lib|exp|dll|so(\\.[0-9])?)$\" -r napi -t 3",
    "build-binaries-electron:linux": "prebuild --backend cmake-js --platform linux --include-regex \"^.*\\.(node|dylib|lib|exp|dll|so(\\.[0-9])?)$\" -r napi -t 3",
    "build-binaries-electron:darwin": "prebuild --backend cmake-js --platform darwin --include-regex \"^.*\\.(node|dylib|lib|exp|dll|so(\\.[0-9])?)$\" -r napi -t 3",
    "build-binaries-electron2": "prebuild --backend cmake-js --platform darwin -a arm64 --include-regex \"^.*\\.(node|dylib|lib|exp|dll|so(\\.[0-9])?)$\" -r napi -t 3",  
    "rebuild": "cmake-js compile"
  },
  "binary": {
    "napi_versions": [
      3
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cmake-js": "^6.3.2",
    "prebuild-install": "^7.1.1",
    "prebuild": "git+https://github.com/ciderapp/prebuild.git",
    "run-script-os": "^1.1.6"
  }
}
